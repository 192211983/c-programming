#include <stdio.h>

// Function to find GCD (Greatest Common Divisor) of two numbers
int gcd(int a, int b) {
    if (b == 0) {
        return a;
    }
    return gcd(b, a % b);
}

// Function to find LCM (Least Common Multiple) of two numbers
int lcm(int a, int b) {
    return (a*b)/gcd(a,b);
}

int main() {
    int num1, num2;
    printf("swetha");
    scanf("%d %d", &num1, &num2);
    printf("LCM of %d and %d is %d\n", num1, num2, lcm(num1, num2));
    return 0;
}
